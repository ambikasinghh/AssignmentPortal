<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegistrationServlet</servlet-name>
        <servlet-class>RegistrationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserAuthentication</servlet-name>
        <servlet-class>UserAuthentication</servlet-class>
        <init-param>
            <param-name>admin-id</param-name>
            <param-value>clg</param-value>
        </init-param>
        <init-param>
            <param-name>admin-password</param-name>
            <param-value>clg123</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>FacultyaccountServlet</servlet-name>
        <servlet-class>FacultyaccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FacultyProfileUpdate</servlet-name>
        <servlet-class>FacultyProfileUpdate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveQuestionServlet</servlet-name>
        <servlet-class>SaveQuestionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubjectListServlet</servlet-name>
        <servlet-class>SubjectListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DIsplayQueSubjectWise</servlet-name>
        <servlet-class>DIsplayQueSubjectWise</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisplayQueSubjectWise</servlet-name>
        <servlet-class>DisplayQueSubjectWise</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EndSession</servlet-name>
        <servlet-class>EndSession</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AssignmentUpload</servlet-name>
        <servlet-class>AssignmentUpload</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrationServlet</servlet-name>
        <url-pattern>/RegistrationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserAuthentication</servlet-name>
        <url-pattern>/UserAuthentication</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FacultyaccountServlet</servlet-name>
        <url-pattern>/FacultyaccountServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FacultyProfileUpdate</servlet-name>
        <url-pattern>/FacultyProfileUpdate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveQuestionServlet</servlet-name>
        <url-pattern>/SaveQuestionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubjectListServlet</servlet-name>
        <url-pattern>/SubjectListServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DIsplayQueSubjectWise</servlet-name>
        <url-pattern>/DIsplayQueSubjectWise</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayQueSubjectWise</servlet-name>
        <url-pattern>/DisplayQueSubjectWise</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EndSession</servlet-name>
        <url-pattern>/EndSession</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AssignmentUpload</servlet-name>
        <url-pattern>/AssignmentUpload</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
